    "version": "0.2.0",
    "configurations": [
        {
			"name": "GWT EvoLudo (localhost)",
            "type": "chrome",
            "request": "launch",
            // "trace": {
            //     "logFile": "${workspaceRoot}/vscode.trace.json",
            // },
            "url": "http://localhost:8888/TestEvoLudoLabs.html",
			"webRoot": "${workspaceRoot}/EvoLudoDev/target/EvoLudoDev-${project.version}",
			"sourceMapPathOverrides": {
				"http://localhost:9876/sourcemaps/evoludoweb/org/evoludo/geom/*" : "${workspaceFolder}/EvoLudoCore/src/main/java/org/evoludo/geom/*",
				"http://localhost:9876/sourcemaps/evoludoweb/org/evoludo/math/*" : "${workspaceFolder}/EvoLudoCore/src/main/java/org/evoludo/math/*",
				"http://localhost:9876/sourcemaps/evoludoweb/org/evoludo/simulator/models/*" : "${workspaceFolder}/EvoLudoCore/src/main/java/org/evoludo/simulator/models/*",
				"http://localhost:9876/sourcemaps/evoludoweb/org/evoludo/simulator/modules/*" : "${workspaceFolder}/EvoLudoCore/src/main/java/org/evoludo/simulator/modules/*",
				"http://localhost:9876/sourcemaps/evoludoweb/org/evoludo/simulator/views/*" : "${workspaceFolder}/EvoLudoCore/src/main/java/org/evoludo/simulator/views/*",
				"http://localhost:9876/sourcemaps/evoludoweb/org/evoludo/simulator/ColorMap.java" : "${workspaceFolder}/EvoLudoCore/src/main/java/org/evoludo/simulator/ColorMap.java",
				"http://localhost:9876/sourcemaps/evoludoweb/org/evoludo/simulator/EvoLudo.java" : "${workspaceFolder}/EvoLudoCore/src/main/java/org/evoludo/simulator/EvoLudo.java",
				"http://localhost:9876/sourcemaps/evoludoweb/org/evoludo/simulator/Geometry.java" : "${workspaceFolder}/EvoLudoCore/src/main/java/org/evoludo/simulator/Geometry.java",
				"http://localhost:9876/sourcemaps/evoludoweb/org/evoludo/simulator/Network.java" : "${workspaceFolder}/EvoLudoCore/src/main/java/org/evoludo/simulator/Network.java",
				"http://localhost:9876/sourcemaps/evoludoweb/org/evoludo/simulator/Network2D.java" : "${workspaceFolder}/EvoLudoCore/src/main/java/org/evoludo/simulator/Network2D.java",
				"http://localhost:9876/sourcemaps/evoludoweb/org/evoludo/simulator/Network3D.java" : "${workspaceFolder}/EvoLudoCore/src/main/java/org/evoludo/simulator/Network3D.java",
				"http://localhost:9876/sourcemaps/evoludoweb/org/evoludo/util/*" : "${workspaceFolder}/EvoLudoCore/src/main/java/org/evoludo/util/*",
				"http://localhost:9876/sourcemaps/evoludoweb/org/evoludo/graphics/*" : "${workspaceFolder}/EvoLudoGWT/src/main/java/org/evoludo/graphics/*",
				"http://localhost:9876/sourcemaps/evoludoweb/org/evoludo/simulator/*" : "${workspaceFolder}/EvoLudoGWT/src/main/java/org/evoludo/simulator/*",
				"http://localhost:9876/sourcemaps/evoludoweb/org/evoludo/ui/*" : "${workspaceFolder}/EvoLudoGWT/src/main/java/org/evoludo/ui/*",
				"http://localhost:9876/sourcemaps/evoludoweb/org/evoludo/EvoLudoWeb.java" : "${workspaceFolder}/EvoLudoGWT/src/main/java/org/evoludo/EvoLudoWeb.java",
			}
        },
        {
			"name": "GWT EvoLudo (10.0.0.10)",
            "type": "chrome",
            "request": "launch",
            "url": "http://10.0.0.10:8888/TestEvoLudoLabs.html",
			"webRoot": "${workspaceRoot}/EvoLudoDev/target/EvoLudoDev-${project.version}",
			"sourceMapPathOverrides": {
				"http://10.0.0.10:9876/sourcemaps/evoludoweb/org/evoludo/geom/*" : "${workspaceFolder}/EvoLudoCore/src/main/java/org/evoludo/geom/*",
				"http://10.0.0.10:9876/sourcemaps/evoludoweb/org/evoludo/math/*" : "${workspaceFolder}/EvoLudoCore/src/main/java/org/evoludo/math/*",
				"http://10.0.0.10:9876/sourcemaps/evoludoweb/org/evoludo/simulator/models/*" : "${workspaceFolder}/EvoLudoCore/src/main/java/org/evoludo/simulator/models/*",
				"http://10.0.0.10:9876/sourcemaps/evoludoweb/org/evoludo/simulator/modules/*" : "${workspaceFolder}/EvoLudoCore/src/main/java/org/evoludo/simulator/modules/*",
				"http://10.0.0.10:9876/sourcemaps/evoludoweb/org/evoludo/simulator/views/*" : "${workspaceFolder}/EvoLudoCore/src/main/java/org/evoludo/simulator/views/*",
				"http://10.0.0.10:9876/sourcemaps/evoludoweb/org/evoludo/simulator/ColorMap.java" : "${workspaceFolder}/EvoLudoCore/src/main/java/org/evoludo/simulator/ColorMap.java",
				"http://10.0.0.10:9876/sourcemaps/evoludoweb/org/evoludo/simulator/EvoLudo.java" : "${workspaceFolder}/EvoLudoCore/src/main/java/org/evoludo/simulator/EvoLudo.java",
				"http://10.0.0.10:9876/sourcemaps/evoludoweb/org/evoludo/simulator/Geometry.java" : "${workspaceFolder}/EvoLudoCore/src/main/java/org/evoludo/simulator/Geometry.java",
				"http://10.0.0.10:9876/sourcemaps/evoludoweb/org/evoludo/simulator/Network.java" : "${workspaceFolder}/EvoLudoCore/src/main/java/org/evoludo/simulator/Network.java",
				"http://10.0.0.10:9876/sourcemaps/evoludoweb/org/evoludo/simulator/Network2D.java" : "${workspaceFolder}/EvoLudoCore/src/main/java/org/evoludo/simulator/Network2D.java",
				"http://10.0.0.10:9876/sourcemaps/evoludoweb/org/evoludo/simulator/Network3D.java" : "${workspaceFolder}/EvoLudoCore/src/main/java/org/evoludo/simulator/Network3D.java",
				"http://10.0.0.10:9876/sourcemaps/evoludoweb/org/evoludo/util/*" : "${workspaceFolder}/EvoLudoCore/src/main/java/org/evoludo/util/*",
				"http://10.0.0.10:9876/sourcemaps/evoludoweb/org/evoludo/graphics/*" : "${workspaceFolder}/EvoLudoGWT/src/main/java/org/evoludo/graphics/*",
				"http://10.0.0.10:9876/sourcemaps/evoludoweb/org/evoludo/simulator/*" : "${workspaceFolder}/EvoLudoGWT/src/main/java/org/evoludo/simulator/*",
				"http://10.0.0.10:9876/sourcemaps/evoludoweb/org/evoludo/ui/*" : "${workspaceFolder}/EvoLudoGWT/src/main/java/org/evoludo/ui/*",
				"http://10.0.0.10:9876/sourcemaps/evoludoweb/org/evoludo/EvoLudoWeb.java" : "${workspaceFolder}/EvoLudoGWT/src/main/java/org/evoludo/EvoLudoWeb.java",
			}
        },
        {
            "name": "JRE Moran EvoLudo",
            "type": "java",
            "request": "launch",
            "mainClass": "${workspaceFolder}/EvoLudoJRE/src/main/java/org/evoludo/simulator/EvoLudoLab.java",
            "args": "--module Moran"
        },
        {
            "name": "JRE 2x2 Evoludo",
            "type": "java",
            "request": "launch",
            "mainClass": "${workspaceFolder}/EvoLudoJRE/src/main/java/org/evoludo/simulator/EvoLudoLab.java",
            "args": "--module 2x2 --model IBS --reportfreq 1 --geometry M --references random 1 --popsize 500 --popupdate async --playerupdate thermal 0.1 --init 0,1 --inittype mutant --mutation 0.0 --paymatrix 0.25,1.25;0,1 --data traits"
        },
        {
            "name": "JRE ePGG EvoLudo",
            "type": "java",
            "request": "launch",
            "mainClass": "${workspaceFolder}/EvoLudoJRE/src/main/java/org/evoludo/simulator/EvoLudoLab.java",
            "args": "--module ePGG --run --delay 100 --pdeN 61x --model PDE --geometry nf --colors red;green;black --reportfreq 1 --pdeD 2,1 --inittype circle --init 1,1,8 --fitnessmap static --cost 1.0 --interest 2.34 --groupsize 8 --deathrate 1.2 --dt 0.1 --pdeL 200"
        },
        {
            "name": "JRE cSD EvoLudo",
            "type": "java",
            "request": "launch",
            "mainClass": "${workspaceFolder}/EvoLudoJRE/src/main/java/org/evoludo/simulator/EvoLudoLab.java",
            "args": "--module cSD --benefitfcn 11 --benefitparams 6,-1.4 --costfcn 1 --costparams 4.56,-1.6 --delay 0 --export cSD-1.plist --generations 1000 --geometry M --init 0.05,0.01 --interactions random 1 --model IBS --mutation 0.1 --mutationsdev 0.01 --mutationtype g --playerupdate thermal 0.1 --popsize 1000 --popupdate async --references random 1 --reportfreq 1 --seed 0"
        },
        {
            "name": "JRE TestEvoludo (generate)",
            "type": "java",
            "request": "launch",
            "mainClass": "${workspaceFolder}/EvoLudoTest/src/main/java/org/evoludo/simulator/exec/TestEvoludo.java",
            "args": "--references ${workspaceFolder}/EvoLudoTest/tests/references --generate ${workspaceFolder}/EvoLudoTest/tests/generators --reports ${workspaceFolder}/EvoLudoTest/tests/reports --compress"
        },
        {
            "name": "JRE TestEvoludo (with current)",
            "type": "java",
            "request": "launch",
            "mainClass": "${workspaceFolder}/EvoLudoTest/src/main/java/org/evoludo/simulator/exec/TestEvoludo.java",
            "args": "--references ${workspaceFolder}/EvoLudoTest/tests/references/current --reports ${workspaceFolder}/EvoLudoTest/tests/reports"
        },
        {
            "name": "JRE TestEvoludo (with clo)",
            "type": "java",
            "request": "launch",
            "mainClass": "${workspaceFolder}/EvoLudoTest/src/main/java/org/evoludo/simulator/exec/TestEvoludo.java",
            "args": "--tests ${workspaceFolder}/EvoLudoTest/tests/generators --references ${workspaceFolder}/EvoLudoTest/tests/references/current --reports ${workspaceFolder}/EvoLudoTest/tests/reports"
        },
    ]
}
