{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
			"name": "GWT EvoLudo",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:8888/TestEvoLudoLabs.html",
			"webRoot": "${workspaceRoot}/war",
			"sourceMapPathOverrides": {
				// "http://127.0.0.1:9876/sourcemaps/evoludoweb/*" : "${workspaceFolder}/src/*",
				"http://127.0.0.1:9876/sourcemaps/evoludoweb/org/evoludo/*" : "${workspaceFolder}/src/org/evoludo/*",
			}
        },
        {
            "name": "JRE Moran EvoLudo",
            "type": "java",
            "request": "launch",
            "mainClass": "${workspaceFolder}/src/org/evoludo/jre/simulator/EvoLudoLab.java",
            "args": "--game Moran"
        },
        {
            "name": "JRE ePGG EvoLudo",
            "type": "java",
            "request": "launch",
            "mainClass": "${workspaceFolder}/src/org/evoludo/jre/simulator/EvoLudoLab.java",
            "args": "--game ePGG --run --delay 100 --pdeN 61x --model PDE --geometry nf --colors red;green;black --reportfreq 1 --diffusion 2,1 --inittype circle --init 1,1,8 --fitnessmap static --basefit 1.0 --selection 1.0 --cost 1.0 --interest 2.34 --groupsize 8 --deathrate 1.2 --dt 0.1 --pdeL 200"
        },
        {
            "name": "JRE TestEvoludo",
            "type": "java",
            "request": "launch",
            "mainClass": "${workspaceFolder}/src/org/evoludo/jre/simulator/exec/TestEvoludo.java",
            "args": "--tests ${workspaceFolder}/test/references --generate ${workspaceFolder}/test/generators --compress"
        },
    ]
}
