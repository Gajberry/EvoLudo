# testing CDL, volunteering in public goods games (group interactions)

# ODE
# base scenario
--module CDL --model ODE --timestep 1 --playerupdate thermal 0.1 --inittype frequency 2,2,6 --interest 3 --groupsize 5 --loner 1 --lonecoop 1 --lonedefect 1 --cost 1 --timestop 1000
# population updates
--module CDL --model ODE --timestep 1 --playerupdate thermal 0.1 --adjusted --inittype frequency 2,2,6 --interest 3 --groupsize 5 --loner 1 --lonecoop 1 --lonedefect 1 --cost 1 --fitnessmap static --timestop 1000
# player updates
--module CDL --model ODE --timestep 1 --playerupdate imitate 0.1 --inittype frequency 2,2,6 --interest 3 --groupsize 5 --loner 1 --lonecoop 1 --lonedefect 1 --cost 1 --timestop 1000
--module CDL --model ODE --timestep 1 --playerupdate best --inittype frequency 2,2,6 --interest 3 --groupsize 5 --loner 1 --lonecoop 1 --lonedefect 1 --cost 1 --timestop 1000
#NOTE heuristic accuracy threshold to trigger convergence for best-response dynamics
--module CDL --model ODE --timestep 1 --playerupdate best-response --inittype frequency 2,2,6 --interest 3 --groupsize 5 --loner 1 --lonecoop 1 --lonedefect 1 --cost 1 --timestop 50
# mutation
--module CDL --model ODE --timestep 1 --playerupdate thermal 0.1 --inittype frequency 2,2,6 --mutations 0.02 other --interest 3 --groupsize 5 --loner 1 --lonecoop 1 --lonedefect 1 --cost 1
# fitness map and selection
--module CDL --model ODE --timestep 1 --playerupdate thermal 0.1 --inittype frequency 2,2,6 --mutations 0.02 other --interest 3 --groupsize 5 --loner 1 --lonecoop 1 --lonedefect 1 --cost 1 --fitnessmap exp 1.0,0.1
#NOTE convergence threshold not reached
--module CDL --model ODE --timestep 1 --playerupdate thermal 0.1 --inittype frequency 2,2,6 --mutations 0.02 other --interest 3 --groupsize 5 --loner 1 --lonecoop 1 --lonedefect 1 --cost 1 --fitnessmap exp 1.0,10.0 --timestop 50

