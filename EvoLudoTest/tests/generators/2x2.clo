# testing traditional 2x2 games

# ODE
# base scenario
--module 2x2 --model ODE --reportfreq 1 --playerupdate thermal 0.1 --init 1,999 --inittype frequency --mutation 0.0 --paymatrix 0.25,1.25;0,1
# population updates
--module 2x2 --model ODE --reportfreq 1 --playerupdate thermal 0.1 --adjusted --init 1,999 --inittype frequency --mutation 0.0 --paymatrix 0.25,1.25;0,1 --fitnessmap static
# player updates
--module 2x2 --model ODE --reportfreq 1 --playerupdate imitate 0.1 --init 1,999 --inittype frequency --mutation 0.0 --paymatrix 0.25,1.25;0,1
--module 2x2 --model ODE --reportfreq 1 --playerupdate best --init 1,999 --inittype frequency --mutation 0.0 --paymatrix 0.25,1.25;0,1
