# testing traditional 2x2 games

# ODE
# base scenario
--module 2x2 --model ODE --timestep 1 --playerupdate thermal 0.1 --inittype frequency 1,999  --paymatrix 0.25,1.25;0,1
# population updates
--module 2x2 --model ODE --timestep 1 --playerupdate thermal 0.1 --adjusted --inittype frequency 1,999  --paymatrix 0.25,1.25;0,1 --fitnessmap static
# player updates
--module 2x2 --model ODE --timestep 1 --playerupdate imitate 0.1 --inittype frequency 1,999  --paymatrix 0.25,1.25;0,1
--module 2x2 --model ODE --timestep 1 --playerupdate best --inittype frequency 1,999  --paymatrix 0.25,1.25;0,1
--module 2x2 --model ODE --timestep 1 --playerupdate best-response --inittype frequency 1,999  --paymatrix 0.25,1.25;0,1
# mutation
--module 2x2 --model ODE --timestep 1 --playerupdate thermal 0.1 --inittype frequency 1,999 --mutations 0.02 other --paymatrix 0.25,1.25;0,1
# fitness map and selection
--module 2x2 --model ODE --timestep 1 --playerupdate thermal 0.1 --inittype frequency 1,999 --mutations 0.02 other --paymatrix 0.25,1.25;0,1 --fitnessmap exp 1.0,0.1
--module 2x2 --model ODE --timestep 1 --playerupdate thermal 0.1 --inittype frequency 1,999 --mutations 0.02 other --paymatrix 0.25,1.25;0,1 --fitnessmap exp 1.0,10.0

