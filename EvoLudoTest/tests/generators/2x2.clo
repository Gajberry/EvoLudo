# mutation
--module 2x2 --model ODE --reportfreq 1 --playerupdate thermal 0.1 --init 1,999 --inittype frequency --mutation 0.02 --paymatrix 0.25,1.25;0,1
# fitness map and selection
--module 2x2 --model ODE --reportfreq 1 --playerupdate thermal 0.1 --init 1,999 --inittype frequency --mutation 0.02 --paymatrix 0.25,1.25;0,1 --fitnessmap exp 1.0,0.1
--module 2x2 --model ODE --reportfreq 1 --playerupdate thermal 0.1 --init 1,999 --inittype frequency --mutation 0.02 --paymatrix 0.25,1.25;0,1 --fitnessmap exp 1.0,10.0

