--game Moran --model SDE --reportfreq 1 --popsize 200 --playerupdate best-response --init 99,1 --inittype frequency --mutation 0.0 --basefit 1.0 --selection 1.0 --fitness 1,1.1
# mutation
--game Moran --model SDE --reportfreq 1 --popsize 200 --playerupdate t0.1 --init 99,1 --inittype frequency --mutation 0.02 --basefit 1.0 --selection 1.0 --fitness 1,1.1 --generations 1000
# fitness map and selection
--game Moran --model SDE --reportfreq 1 --popsize 200 --playerupdate t0.1 --init 99,1 --inittype frequency --mutation 0.02 --basefit 1.0 --selection 0.1 --fitness 1,1.1 --fitnessmap exp --generations 1000
--game Moran --model SDE --reportfreq 1 --popsize 200 --playerupdate t0.1 --init 99,1 --inittype frequency --mutation 0.02 --basefit 1.0 --selection 10.0 --fitness 1,1.1 --fitnessmap exp --generations 1000

