# testing original Moran process (fixed fitness, fixed population size)

# ODE
# base scenario
--module Moran --model ODE --reportfreq 1 --playerupdate thermal 0.1 --init 999,1 --inittype frequency --mutation 0.0 --fitness 1,1.1
# population updates
--module Moran --model ODE --reportfreq 1 --playerupdate thermal 0.1 --adjusted --init 999,1 --inittype frequency --mutation 0.0 --fitness 1,1.1
# player updates
--module Moran --model ODE --reportfreq 1 --playerupdate imitate 0.1 --init 999,1 --inittype frequency --mutation 0.0 --fitness 1,1.1
--module Moran --model ODE --reportfreq 1 --playerupdate best --init 999,1 --inittype frequency --mutation 0.0 --fitness 1,1.1
--module Moran --model ODE --reportfreq 1 --playerupdate best-response --init 999,1 --inittype frequency --mutation 0.0 --fitness 1,1.1
# mutation
--module Moran --model ODE --reportfreq 1 --playerupdate thermal 0.1 --init 999,1 --inittype frequency --mutation 0.02 --fitness 1,1.1
# fitness map and selection
--module Moran --model ODE --reportfreq 1 --playerupdate thermal 0.1 --init 999,1 --inittype frequency --mutation 0.02 --fitness 1,1.1 --fitnessmap exp 1.0,0.1
--module Moran --model ODE --reportfreq 1 --playerupdate thermal 0.1 --init 999,1 --inittype frequency --mutation 0.02 --fitness 1,1.1 --fitnessmap exp 1.0,10.0

# SDE
# base scenario
--module Moran --model SDE --reportfreq 1 --popsize 200 --playerupdate thermal 0.1 --init 99,1 --inittype frequency --mutation 0.0 --fitness 1,1.1
# population updates
# adjusted not yet implemented
#--module Moran --model SDE --reportfreq 1 --popsize 200 --playerupdate thermal 0.1 --adjusted --init 99,1 --inittype frequency --mutation 0.0 --fitness 1,1.1
# player updates
--module Moran --model SDE --reportfreq 1 --popsize 200 --playerupdate imitate 0.1 --init 99,1 --inittype frequency --mutation 0.0 --fitness 1,1.1
--module Moran --model SDE --reportfreq 1 --popsize 200 --playerupdate best --init 99,1 --inittype frequency --mutation 0.0 --fitness 1,1.1
--module Moran --model SDE --reportfreq 1 --popsize 200 --playerupdate best-response --init 99,1 --inittype frequency --mutation 0.0 --fitness 1,1.1
# mutation
--module Moran --model SDE --reportfreq 1 --popsize 200 --playerupdate thermal 0.1 --init 99,1 --inittype frequency --mutation 0.02 --fitness 1,1.1 --generations 1000
# fitness map and selection
--module Moran --model SDE --reportfreq 1 --popsize 200 --playerupdate thermal 0.1 --init 99,1 --inittype frequency --mutation 0.02 --fitness 1,1.1 --fitnessmap exp 1.0,0.1 --generations 1000
--module Moran --model SDE --reportfreq 1 --popsize 200 --playerupdate thermal 0.1 --init 99,1 --inittype frequency --mutation 0.02 --fitness 1,1.1 --fitnessmap exp 1.0,10.0 --generations 1000

