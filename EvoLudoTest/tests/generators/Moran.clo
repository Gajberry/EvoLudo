# testing original Moran process (fixed fitness, fixed population size)

# ODE
# base scenario
--module Moran --model ODE --timestep 1 --playerupdate thermal 0.1 --inittype frequency 999,1 --fitness 1,1.1
# population updates
--module Moran --model ODE --timestep 1 --playerupdate thermal 0.1 --adjusted --inittype frequency 999,1 --fitness 1,1.1
# player updates
--module Moran --model ODE --timestep 1 --playerupdate imitate 0.1 --inittype frequency 999,1 --fitness 1,1.1
--module Moran --model ODE --timestep 1 --playerupdate best --inittype frequency 999,1 --fitness 1,1.1
--module Moran --model ODE --timestep 1 --playerupdate best-response --inittype frequency 999,1 --fitness 1,1.1
# mutation
--module Moran --model ODE --timestep 1 --playerupdate thermal 0.1 --inittype frequency 999,1 --mutations 0.02 other --fitness 1,1.1
# fitness map and selection
--module Moran --model ODE --timestep 1 --playerupdate thermal 0.1 --inittype frequency 999,1 --mutations 0.02 other --fitness 1,1.1 --fitnessmap exp 1.0,0.1
--module Moran --model ODE --timestep 1 --playerupdate thermal 0.1 --inittype frequency 999,1 --mutations 0.02 other --fitness 1,1.1 --fitnessmap exp 1.0,10.0

