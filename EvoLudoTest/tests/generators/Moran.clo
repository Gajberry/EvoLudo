# mutation
--module Moran --model PDE --reportfreq 1 --geometry n --playerupdate thermal 0.1 --init 99,1 --inittype random --mutation 0.02 --fitness 1,1.1
# fitness map and selection
--module Moran --model PDE --reportfreq 1 --geometry n --playerupdate thermal 0.1 --init 99,1 --inittype random --mutation 0.02 --fitnessmap exp 1.0,0.1 --fitness 1,1.1
