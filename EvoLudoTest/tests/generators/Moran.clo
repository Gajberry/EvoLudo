# player updates
--module Moran --model SDE --timestep 1 --popsize 200 --playerupdate imitate 0.1 --inittype frequency 99,1 --fitness 1,1.1
--module Moran --model SDE --timestep 1 --popsize 200 --playerupdate best --inittype frequency 99,1 --fitness 1,1.1
--module Moran --model SDE --timestep 1 --popsize 200 --playerupdate best-response --inittype frequency 99,1 --fitness 1,1.1
# mutation
--module Moran --model SDE --timestep 1 --popsize 200 --playerupdate thermal 0.1 --inittype frequency 99,1 --mutations 0.02 other --fitness 1,1.1 --timestop 1000
# fitness map and selection
--module Moran --model SDE --timestep 1 --popsize 200 --playerupdate thermal 0.1 --inittype frequency 99,1 --mutations 0.02 other --fitness 1,1.1 --fitnessmap exp 1.0,0.1 --timestop 1000
--module Moran --model SDE --timestep 1 --popsize 200 --playerupdate thermal 0.1 --inittype frequency 99,1 --mutations 0.02 other --fitness 1,1.1 --fitnessmap exp 1.0,10.0 --timestop 1000

