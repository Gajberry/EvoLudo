# testing original Moran process (fixed fitness, fixed population size)

# ODE
# base scenario
--game Moran --model ODE --reportfreq 1 --playerupdate t0.1 --init 999,1 --inittype frequency --mutation 0.0 --basefit 1.0 --selection 1.0 --fitness 1,1.1
# population updates
--game Moran --model ODE --reportfreq 1 --playerupdate t0.1 --adjusted --init 999,1 --inittype frequency --mutation 0.0 --basefit 1.0 --selection 1.0 --fitness 1,1.1
# player updates
--game Moran --model ODE --reportfreq 1 --playerupdate i0.1 --init 999,1 --inittype frequency --mutation 0.0 --basefit 1.0 --selection 1.0 --fitness 1,1.1
--game Moran --model ODE --reportfreq 1 --playerupdate best --init 999,1 --inittype frequency --mutation 0.0 --basefit 1.0 --selection 1.0 --fitness 1,1.1
--game Moran --model ODE --reportfreq 1 --playerupdate best-response --init 999,1 --inittype frequency --mutation 0.0 --basefit 1.0 --selection 1.0 --fitness 1,1.1
# mutation
--game Moran --model ODE --reportfreq 1 --playerupdate t0.1 --init 999,1 --inittype frequency --mutation 0.02 --basefit 1.0 --selection 1.0 --fitness 1,1.1
# fitness map and selection
--game Moran --model ODE --reportfreq 1 --playerupdate t0.1 --init 999,1 --inittype frequency --mutation 0.02 --basefit 1.0 --selection 0.1 --fitness 1,1.1 --fitnessmap exp
--game Moran --model ODE --reportfreq 1 --playerupdate t0.1 --init 999,1 --inittype frequency --mutation 0.02 --basefit 1.0 --selection 10.0 --fitness 1,1.1 --fitnessmap exp

# SDE
# base scenario
